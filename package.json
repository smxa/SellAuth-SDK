{
  "name": "@wtfservices/sellauth-utils",
  "version": "0.5.3",
  "description": "TypeScript SDK for the SellAuth API (resources, advanced client, pagination, middleware).",
  "license": "MIT",
  "author": "WTF Services <business@illegal.wtf>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smxa/SellAuth-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/smxa/SellAuth-SDK/issues"
  },
  "homepage": "https://github.com/smxa/SellAuth-SDK#readme",
  "keywords": [
    "sellauth",
    "sdk",
    "api",
    "payments",
    "typescript",
    "middleware",
    "pagination"
  ],
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./advanced": {
      "types": "./dist/sdk/advanced.d.ts",
      "import": "./dist/sdk/advanced.mjs",
      "require": "./dist/sdk/advanced.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts src/sdk/advanced.ts --format cjs,esm --dts --sourcemap --clean --out-dir dist",
    "test:build": "node -e \"require('./dist');import('./dist/index.mjs')\"",
    "prepare": "pnpm run build",
    "lint": "eslint . --ext .ts",
    "example": "tsx examples/usage.ts",
    "example:advanced": "tsx examples/advanced.ts",
    "example:caching": "tsx examples/caching-middleware.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "@eslint/js": "^9.34.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.15.0"
}
