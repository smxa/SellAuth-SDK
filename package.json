{
  "name": "@wtfservices/sellauth-utils",
  "version": "0.2.1",
  "description": "TypeScript SDK for the SellAuth API (resources, advanced client, pagination, middleware).",
  "license": "MIT",
  "author": "WTF Services <business@illegal.wtf>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smxa/SellAuth-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/smxa/SellAuth-SDK/issues"
  },
  "homepage": "https://github.com/smxa/SellAuth-SDK#readme",
  "keywords": [
    "sellauth",
    "sdk",
    "api",
    "payments",
    "typescript",
    "middleware",
    "pagination"
  ],
  "type": "commonjs",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist dist-cjs",
    "build:tsup": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean --out-dir dist",
    "build": "npm run build:tsup",
    "_old:build:cjs": "tsc -p tsconfig.cjs.json",
    "_old:build:dual": "rm -rf dist dist-cjs && npm run build:esm && npm run build:cjs && node scripts/patch-esm.mjs && cp dist-cjs/index.js dist/index.cjs",
    "_old:build": "npm run build:dual",
    "prepare": "npm run build:tsup",
    "lint": "eslint . --ext .ts",
    "example": "tsx examples/usage.ts",
    "example:advanced": "tsx examples/advanced.ts",
    "example:caching": "tsx examples/caching-middleware.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18"
  }
}
